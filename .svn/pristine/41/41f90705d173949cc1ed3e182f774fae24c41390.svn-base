      nGet5 := 0
      oGet8:SetFocus()
    Case Empty(cGet3) .and. nGet5 > 0 .And. ZZ5->ZZ5_CLOTE == "L"
      Alert("Não pode deixar a Lote em Branco")
      nGet5 := 0
      oGet3:SetFocus()
