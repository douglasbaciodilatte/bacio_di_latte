static Function fSoma(nCampo)
Default nCampo := 1
  nAux1 := ZZ5->ZZ5_QTD11
  nAux2 := ZZ5->ZZ5_QTD21

  if Alltrim (cGet9) $ "700003/800003"
      
      //if nCampo == 1
        if nGet12 <> 0 .or. nAux1 == 0 .or. nGet14 <> 0 .or. nAux2 == 0
          nGet5 := nGet12
          nGet7 := nGet14
        else
          nGet5 := nAux1
          nGet7 := nAux2
        Endif
      //endif
/*
      if nCampo == 2 
        if nget14 <> 0  .or. nAux2 == 0
          nGet7 := nGet14
        else
          nGet7 := nAux2
        Endif
      Endif*/
  Else
    if nCampo == 1
      if nGet12 <> 0 
        nGet5 := nGet12 + nAux1
      else
        nGet5 := nAux1
      Endif
    endif

    if nCampo == 2  
      if nget14 <> 0 
        nGet7 := nGet14 + nAux2
      else
        nGet7 := nAux2
      Endif
    endif

  Endif

Return .T.

