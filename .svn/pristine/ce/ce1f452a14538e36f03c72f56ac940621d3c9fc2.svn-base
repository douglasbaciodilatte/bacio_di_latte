
User Function A650LOCA()

Local cRetLocal 

	//Verifca se o produto diferente de 'MO'(mão de Obrra) e não faz parte do grupo 0198
 	If  Alltrim(SB1->B1_TIPO)<>'MO' .And. SC2->C2_FILIAL $ "0031"
		cRetLocal := '700002'
	ElseIf Alltrim(SB1->B1_TIPO)<>'MO' .And. SC2->C2_FILIAL $ "0072"	
		cRetLocal := '700023'	
 	EndIf
 
Return cRetLocal 
