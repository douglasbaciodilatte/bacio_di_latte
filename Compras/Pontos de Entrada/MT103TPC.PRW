User Function MT103TPC()

Local cTes := PARAMIXB[1]// Tratamentos do cliente
Local i        := 0


DbSelectArea("SX6")
DbSetOrder(1)

While SX6->(DbSeek(xFilial("SX6")+"MV_TESPCB"+StrZero(++i,1))) 
                                                                                           
   IF !EMPTY(GetMV("MV_TESPCB"+StrZero(i,1)))
   		cTes += ","+GetMV("MV_TESPCB"+StrZero(i,1))    
   END  
   
 
End   


Return cTes
